[{"url": "https://api.github.com/repos/blackbeam/mysql_async/issues/213", "repository_url": "https://api.github.com/repos/blackbeam/mysql_async", "labels_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/213/labels{/name}", "comments_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/213/comments", "events_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/213/events", "html_url": "https://github.com/blackbeam/mysql_async/pull/213", "id": 1445931745, "node_id": "PR_kwDOBDZuQM5CujYP", "number": 213, "title": "Turn get_conn waiting queue into FIFO queue.", "user": {"login": "cloneable", "id": 9938970, "node_id": "MDQ6VXNlcjk5Mzg5NzA=", "avatar_url": "https://avatars.githubusercontent.com/u/9938970?v=4", "gravatar_id": "", "url": "https://api.github.com/users/cloneable", "html_url": "https://github.com/cloneable", "followers_url": "https://api.github.com/users/cloneable/followers", "following_url": "https://api.github.com/users/cloneable/following{/other_user}", "gists_url": "https://api.github.com/users/cloneable/gists{/gist_id}", "starred_url": "https://api.github.com/users/cloneable/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/cloneable/subscriptions", "organizations_url": "https://api.github.com/users/cloneable/orgs", "repos_url": "https://api.github.com/users/cloneable/repos", "events_url": "https://api.github.com/users/cloneable/events{/privacy}", "received_events_url": "https://api.github.com/users/cloneable/received_events", "type": "User", "user_view_type": "public", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 0, "created_at": "2022-11-11T19:38:53Z", "updated_at": "2022-11-12T20:43:50Z", "closed_at": "2022-11-12T20:43:50Z", "author_association": "CONTRIBUTOR", "active_lock_reason": null, "draft": false, "pull_request": {"url": "https://api.github.com/repos/blackbeam/mysql_async/pulls/213", "html_url": "https://github.com/blackbeam/mysql_async/pull/213", "diff_url": "https://github.com/blackbeam/mysql_async/pull/213.diff", "patch_url": "https://github.com/blackbeam/mysql_async/pull/213.patch", "merged_at": "2022-11-12T20:43:50Z"}, "body": "* Use a `BinaryHeap` for `Exchange::waiting`.\r\n* Wrap `GetConn` wakers inside the new `QueuedWaker`.\r\n* Use the new `QueueId` which is an atomic u64 counter\r\n  to prioritize the enqueued and re-enqueued wakers. \r\n\r\nThe AtomicU64 may theoretically wrap around, though should\r\nnot matter in practice. Should be documented.\r\n\r\nCloses #212", "reactions": {"url": "https://api.github.com/repos/blackbeam/mysql_async/issues/213/reactions", "total_count": 0, "+1": 0, "-1": 0, "laugh": 0, "hooray": 0, "confused": 0, "heart": 0, "rocket": 0, "eyes": 0}, "timeline_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/213/timeline", "performed_via_github_app": null, "state_reason": null, "score": 1.0}, {"url": "https://api.github.com/repos/blackbeam/mysql_async/issues/209", "repository_url": "https://api.github.com/repos/blackbeam/mysql_async", "labels_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/209/labels{/name}", "comments_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/209/comments", "events_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/209/events", "html_url": "https://github.com/blackbeam/mysql_async/pull/209", "id": 1335202344, "node_id": "PR_kwDOBDZuQM48_Ojy", "number": 209, "title": "Implemented rustls support, fixes #195", "user": {"login": "TobiasDeBruijn", "id": 28569170, "node_id": "MDQ6VXNlcjI4NTY5MTcw", "avatar_url": "https://avatars.githubusercontent.com/u/28569170?v=4", "gravatar_id": "", "url": "https://api.github.com/users/TobiasDeBruijn", "html_url": "https://github.com/TobiasDeBruijn", "followers_url": "https://api.github.com/users/TobiasDeBruijn/followers", "following_url": "https://api.github.com/users/TobiasDeBruijn/following{/other_user}", "gists_url": "https://api.github.com/users/TobiasDeBruijn/gists{/gist_id}", "starred_url": "https://api.github.com/users/TobiasDeBruijn/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/TobiasDeBruijn/subscriptions", "organizations_url": "https://api.github.com/users/TobiasDeBruijn/orgs", "repos_url": "https://api.github.com/users/TobiasDeBruijn/repos", "events_url": "https://api.github.com/users/TobiasDeBruijn/events{/privacy}", "received_events_url": "https://api.github.com/users/TobiasDeBruijn/received_events", "type": "User", "user_view_type": "public", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 6, "created_at": "2022-08-10T21:07:14Z", "updated_at": "2022-11-12T14:36:52Z", "closed_at": "2022-11-12T14:36:51Z", "author_association": "CONTRIBUTOR", "active_lock_reason": null, "draft": false, "pull_request": {"url": "https://api.github.com/repos/blackbeam/mysql_async/pulls/209", "html_url": "https://github.com/blackbeam/mysql_async/pull/209", "diff_url": "https://github.com/blackbeam/mysql_async/pull/209.diff", "patch_url": "https://github.com/blackbeam/mysql_async/pull/209.patch", "merged_at": "2022-11-12T14:36:51Z"}, "body": "This PR adds support for rustls, and makes using a TLS implementation optional.\r\n\r\nI've inspired this commit purely on https://github.com/blackbeam/rust-mysql-simple/pull/295 and the documentation for the various crates. I'm not familiar with the workings of rustls or native-tls though.\r\n\r\nThis PR adds the following features:\r\n - `flate2-zlib` A `flate` backend is needed to compile this crate. Users of `rustls` will turn off the default features when using it,\r\n hence causing compilation issues. The usual fix for this is for the user to include flate2 in their own Cargo.toml, though this seems suboptimal. So I've made the default flate2 backend it's own feature for ease of use.\r\n- `native-tls-tls` Yes, the double `tls` is intentional. This enabls the native-tls backend and the required crates for it.\r\n- `rustls-tls` Enables the rustls-tls backend\r\n\r\nHopefully this all is to spec.\r\nThanks!", "reactions": {"url": "https://api.github.com/repos/blackbeam/mysql_async/issues/209/reactions", "total_count": 2, "+1": 2, "-1": 0, "laugh": 0, "hooray": 0, "confused": 0, "heart": 0, "rocket": 0, "eyes": 0}, "timeline_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/209/timeline", "performed_via_github_app": null, "state_reason": null, "score": 1.0}, {"url": "https://api.github.com/repos/blackbeam/mysql_async/issues/206", "repository_url": "https://api.github.com/repos/blackbeam/mysql_async", "labels_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/206/labels{/name}", "comments_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/206/comments", "events_url": "https://api.github.com/repos/blackbeam/mysql_async/issues/206/events", "html_url": "https://github.com/blackbeam/mysql_async/pull/206", "id": 1257034325, "node_id": "PR_kwDOBDZuQM446jnP", "number": 206, "title": "Relax prep() to take impl AsQuery", "user": {"login": "glittershark", "id": 1481027, "node_id": "MDQ6VXNlcjE0ODEwMjc=", "avatar_url": "https://avatars.githubusercontent.com/u/1481027?v=4", "gravatar_id": "", "url": "https://api.github.com/users/glittershark", "html_url": "https://github.com/glittershark", "followers_url": "https://api.github.com/users/glittershark/followers", "following_url": "https://api.github.com/users/glittershark/following{/other_user}", "gists_url": "https://api.github.com/users/glittershark/gists{/gist_id}", "starred_url": "https://api.github.com/users/glittershark/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/glittershark/subscriptions", "organizations_url": "https://api.github.com/users/glittershark/orgs", "repos_url": "https://api.github.com/users/glittershark/repos", "events_url": "https://api.github.c