[{"url": "https://api.github.com/repos/apgoetz/jolly/issues/5", "repository_url": "https://api.github.com/repos/apgoetz/jolly", "labels_url": "https://api.github.com/repos/apgoetz/jolly/issues/5/labels{/name}", "comments_url": "https://api.github.com/repos/apgoetz/jolly/issues/5/comments", "events_url": "https://api.github.com/repos/apgoetz/jolly/issues/5/events", "html_url": "https://github.com/apgoetz/jolly/pull/5", "id": 1514810638, "node_id": "PR_kwDOGbnHx85GZ5vz", "number": 5, "title": "Modified windows subshell spawning to avoid creating console window", "user": {"login": "apgoetz", "id": 1356587, "node_id": "MDQ6VXNlcjEzNTY1ODc=", "avatar_url": "https://avatars.githubusercontent.com/u/1356587?v=4", "gravatar_id": "", "url": "https://api.github.com/users/apgoetz", "html_url": "https://github.com/apgoetz", "followers_url": "https://api.github.com/users/apgoetz/followers", "following_url": "https://api.github.com/users/apgoetz/following{/other_user}", "gists_url": "https://api.github.com/users/apgoetz/gists{/gist_id}", "starred_url": "https://api.github.com/users/apgoetz/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/apgoetz/subscriptions", "organizations_url": "https://api.github.com/users/apgoetz/orgs", "repos_url": "https://api.github.com/users/apgoetz/repos", "events_url": "https://api.github.com/users/apgoetz/events{/privacy}", "received_events_url": "https://api.github.com/users/apgoetz/received_events", "type": "User", "user_view_type": "public", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 0, "created_at": "2022-12-30T20:39:17Z", "updated_at": "2022-12-30T20:46:57Z", "closed_at": "2022-12-30T20:46:53Z", "author_association": "OWNER", "active_lock_reason": null, "draft": false, "pull_request": {"url": "https://api.github.com/repos/apgoetz/jolly/pulls/5", "html_url": "https://github.com/apgoetz/jolly/pull/5", "diff_url": "https://github.com/apgoetz/jolly/pull/5.diff", "patch_url": "https://github.com/apgoetz/jolly/pull/5.patch", "merged_at": "2022-12-30T20:46:53Z"}, "body": "When running a command on windows, windows needs to decide whether or not to create a console window that appears.\r\n\r\nSince Jolly is a GUI application that does not have a console, this\r\n was appearing whenever system entries were executed.\r\n\r\nHowever, this was only happening for release builds, since debug build still create a console to show log output. It was further confused by the fact that jolly is usually developed from the command line, so the fact that debug builds were popping up a console was also not noticed, since launching Jolly from the command prompt would hide this extra window.\r\n\r\nThis changes the logic so that windows are never spawned with a console attached, in debug or release mode.", "reactions": {"url": "https://api.github.com/repos/apgoetz/jolly/issues/5/reactions", "total_count": 0, "+1": 0, "-1": 0, "laugh": 0, "hooray": 0, "confused": 0, "heart": 0, "rocket": 0, "eyes": 0}, "timeline_url": "https://api.github.com/repos/apgoetz/jolly/issues/5/timeline", "performed_via_github_app": null, "state_reason": null, "score": 1.0}, {"url": "https://api.github.com/repos/apgoetz/jolly/issues/4", "repository_url": "https://api.github.com/repos/apgoetz/jolly", "labels_url": "https://api.github.com/repos/apgoetz/jolly/issues/4/labels{/name}", "comments_url": "https://api.github.com/repos/apgoetz/jolly/issues/4/comments", "events_url": "https://api.github.com/repos/apgoetz/jolly/issues/4/events", "html_url": "https://github.com/apgoetz/jolly/pull/4", "id": 1510254661, "node_id": "PR_kwDOGbnHx85GKptq", "number": 4, "title": "Add Release CI", "user": {"login": "apgoetz", "id": 1356587, "node_id": "MDQ6VXNlcjEzNTY1ODc=", "avatar_url": "https://avatars.githubusercontent.com/u/1356587?v=4", "gravatar_id": "", "url": "https://api.github.com/users/apgoetz", "html_url": "https://github.com/apgoetz", "follower