[{"url": "https://api.github.com/repos/benashford/redis-async-rs/issues/98", "repository_url": "https://api.github.com/repos/benashford/redis-async-rs", "labels_url": "https://api.github.com/repos/benashford/redis-async-rs/issues/98/labels{/name}", "comments_url": "https://api.github.com/repos/benashford/redis-async-rs/issues/98/comments", "events_url": "https://api.github.com/repos/benashford/redis-async-rs/issues/98/events", "html_url": "https://github.com/benashford/redis-async-rs/pull/98", "id": 3387611847, "node_id": "PR_kwDOBYUIMM6nDPof", "number": 98, "title": "Updated and resilient networking", "user": {"login": "benashford", "id": 465969, "node_id": "MDQ6VXNlcjQ2NTk2OQ==", "avatar_url": "https://avatars.githubusercontent.com/u/465969?v=4", "gravatar_id": "", "url": "https://api.github.com/users/benashford", "html_url": "https://github.com/benashford", "followers_url": "https://api.github.com/users/benashford/followers", "following_url": "https://api.github.com/users/benashford/following{/other_user}", "gists_url": "https://api.github.com/users/benashford/gists{/gist_id}", "starred_url": "https://api.github.com/users/benashford/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/benashford/subscriptions", "organizations_url": "https://api.github.com/users/benashford/orgs", "repos_url": "https://api.github.com/users/benashford/repos", "events_url": "https://api.github.com/users/benashford/events{/privacy}", "received_events_url": "https://api.github.com/users/benashford/received_events", "type": "User", "user_view_type": "public", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 0, "created_at": "2025-09-05T13:43:05Z", "updated_at": "2025-09-08T10:33:51Z", "closed_at": "2025-09-08T10:33:45Z", "author_association": "OWNER", "active_lock_reason": null, "draft": false, "pull_request": {"url": "https://api.github.com/repos/benashford/redis-async-rs/pulls/98", "html_url": "https://github.com/benashford/redis-async-rs/pull/98", "diff_url": "https://github.com/benashford/redis-async-rs/pull/98.diff", "patch_url": "https://github.com/benashford/redis-async-rs/pull/98.patch", "merged_at": "2025-09-08T10:33:45Z"}, "body": "Minor modification to the way connections happen.\r\n\r\nNow will retry more than once, before marking the connection as failed, with a smaller timeout. This is now also customisable. It will stay in a Connecting state while this happens.\r\n\r\nThis is to aide resilience in unreliable environments, e.g. Kubernetes clusters and other such situations.", "reactions": {"url": "https://api.github.com/repos/benashford/redis-async-rs/issues/98/reactions", "total_count": 0, "+1": 0, "-1": 0, "laugh": 0, "hooray": 0, "confused": 0, "heart": 0, "rocket": 0, "eyes": 0}, "timeline_url": "https://api.github.com/repos/benashford/redis-async-rs/issues/98/timeline", "performed_via_github_app": null, "state_reason": null, "score": 1.0}, {"url": "https://api.github.com/repos/benashford/redis-async-rs/issues/95", "repository_url": "https://api.github.com/repos/benashford/redis-async-rs", "labels_url": "https://api.github.com/repos/benashford/redis-async-rs/issues/95/labels{/name}", "comments_url": "https://api.github.com/repos/benashford/redis-async-rs/issues/95/comments", "events_url": "https://api.github.com/repos/benashford/redis-async-rs/issues/95/events", "html_url": "https://github.com/benashford/redis-async-rs/pull/95", "id": 2300903452, "node_id": "PR_kwDOBYUIMM5vsZte", "number": 95, "title": "Upgrade Rustls", "user": {"login": "benashford", "id": 465969, "node_id": "MDQ6VXNlcjQ2NTk2OQ==", "avatar_url": "https://avatars.githubusercontent.com/u/465969?v=4", "gravatar_id": "", "url": "https://api.github.com/users/benashford", "html_url": "https://github.com/benashford", "followers_url": "https://api.github.com/users/benashford/followers", "following_url": "https://api.github.com/users/benashford/following{/other