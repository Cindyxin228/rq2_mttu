[{"url": "https://api.github.com/repos/borntyping/rust-simple_logger/issues/41", "repository_url": "https://api.github.com/repos/borntyping/rust-simple_logger", "labels_url": "https://api.github.com/repos/borntyping/rust-simple_logger/issues/41/labels{/name}", "comments_url": "https://api.github.com/repos/borntyping/rust-simple_logger/issues/41/comments", "events_url": "https://api.github.com/repos/borntyping/rust-simple_logger/issues/41/events", "html_url": "https://github.com/borntyping/rust-simple_logger/pull/41", "id": 1046645520, "node_id": "PR_kwDOAdyr1M4uL_i-", "number": 41, "title": "Exchanged `chrono` Dependency for Equal Implementation Using The Still-Maintained `time` Crate", "user": {"login": "noahcoetsee", "id": 12777708, "node_id": "MDQ6VXNlcjEyNzc3NzA4", "avatar_url": "https://avatars.githubusercontent.com/u/12777708?v=4", "gravatar_id": "", "url": "https://api.github.com/users/noahcoetsee", "html_url": "https://github.com/noahcoetsee", "followers_url": "https://api.github.com/users/noahcoetsee/followers", "following_url": "https://api.github.com/users/noahcoetsee/following{/other_user}", "gists_url": "https://api.github.com/users/noahcoetsee/gists{/gist_id}", "starred_url": "https://api.github.com/users/noahcoetsee/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/noahcoetsee/subscriptions", "organizations_url": "https://api.github.com/users/noahcoetsee/orgs", "repos_url": "https://api.github.com/users/noahcoetsee/repos", "events_url": "https://api.github.com/users/noahcoetsee/events{/privacy}", "received_events_url": "https://api.github.com/users/noahcoetsee/received_events", "type": "User", "user_view_type": "public", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 15, "created_at": "2021-11-07T02:46:12Z", "updated_at": "2021-11-25T14:43:01Z", "closed_at": "2021-11-08T14:33:37Z", "author_association": "CONTRIBUTOR", "active_lock_reason": null, "draft": false, "pull_request": {"url": "https://api.github.com/repos/borntyping/rust-simple_logger/pulls/41", "html_url": "https://github.com/borntyping/rust-simple_logger/pull/41", "diff_url": "https://github.com/borntyping/rust-simple_logger/pull/41.diff", "patch_url": "https://github.com/borntyping/rust-simple_logger/pull/41.patch", "merged_at": "2021-11-08T14:33:37Z"}, "body": "The reason for this pull request is the recent security advisory, [RUSTSEC-2020-0159](https://rustsec.org/advisories/RUSTSEC-2020-0159). This issue detected a potential segfault in `localtime_r` invocations, and given the `chrono` crate's lack of maintenance, the `time` crate, which fixed the issue, is now the preferred crate for datetime offsets and formatting.\r\n\r\nThe format used is still identical to the one achieved via `chrono`, but this implementation also prevents the issues caused by RUSTSEC-2020-0159, which were failing CI/CL actions in many repositories dependent upon `rust-simple_logger`.\r\n\r\nThe changes should still work in a no_alloc, no_std situation as expected.\r\n\r\n(note: yes I see that I did a typo and put RUSTSEC-2021 instead of RUSTSEC-2020 in the commit. oops)", "reactions": {"url": "https://api.github.com/repos/borntyping/rust-simple_logger/issues/41/reactions", "total_count": 1, "+1": 0, "-1": 0, "laugh": 0, "hooray": 0, "confused": 0, "heart": 1, "rocket": 0, "eyes": 0}, "timeline_url": "https://api.github.com/repos/borntyping/rust-simple_logger/issues/41/timeline", "performed_via_github_app": null, "state_reason": null, "score": 1.0}, {"url": "https://api.github.com/repos/borntyping/rust-simple_logger/issues/38", "repository_url": "https://api.github.com/repos/borntyping/rust-simple_logger", "labels_url": "https://api.github.com/repos/borntyping/rust-simple_logger/issues/38/labels{/name}", "comments_url": "https://api.github.com/repos/borntyping/rust-simple_logger/issues/38/comments", "events_url": "https://api.github.com/repos/borntyping/rust-s