hwind.", "reactions": {"url": "https://api.github.com/repos/alianse777/darknet-rust/issues/14/reactions", "total_count": 0, "+1": 0, "-1": 0, "laugh": 0, "hooray": 0, "confused": 0, "heart": 0, "rocket": 0, "eyes": 0}, "timeline_url": "https://api.github.com/repos/alianse777/darknet-rust/issues/14/timeline", "performed_via_github_app": null, "state_reason": null, "score": 1.0}, {"url": "https://api.github.com/repos/alianse777/darknet-rust/issues/12", "repository_url": "https://api.github.com/repos/alianse777/darknet-rust", "labels_url": "https://api.github.com/repos/alianse777/darknet-rust/issues/12/labels{/name}", "comments_url": "https://api.github.com/repos/alianse777/darknet-rust/issues/12/comments", "events_url": "https://api.github.com/repos/alianse777/darknet-rust/issues/12/events", "html_url": "https://github.com/alianse777/darknet-rust/pull/12", "id": 785129798, "node_id": "MDExOlB1bGxSZXF1ZXN0NTU0MjA3MDcz", "number": 12, "title": "Revert \"Expose `enable-cudnn` feature\"", "user": {"login": "alianse777", "id": 20813404, "node_id": "MDQ6VXNlcjIwODEzNDA0", "avatar_url": "https://avatars.githubusercontent.com/u/20813404?v=4", "gravatar_id": "", "url": "https://api.github.com/users/alianse777", "html_url": "https://github.com/alianse777", "followers_url": "https://api.github.com/users/alianse777/followers", "following_url": "https://api.github.com/users/alianse777/following{/other_user}", "gists_url": "https://api.github.com/users/alianse777/gists{/gist_id}", "starred_url": "https://api.github.com/users/alianse777/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/alianse777/subscriptions", "organizations_url": "https://api.github.com/users/alianse777/orgs", "repos_url": "https://api.github.com/users/alianse777/repos", "events_url": "https://api.github.com/users/alianse777/events{/privacy}", "received_events_url": "https://api.github.com/users/alianse777/received_events", "type": "User", "user_view_type": "public", "site_admin": false}