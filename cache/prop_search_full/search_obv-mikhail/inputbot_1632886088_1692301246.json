[{"url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/71", "repository_url": "https://api.github.com/repos/obv-mikhail/InputBot", "labels_url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/71/labels{/name}", "comments_url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/71/comments", "events_url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/71/events", "html_url": "https://github.com/obv-mikhail/InputBot/pull/71", "id": 1581438519, "node_id": "PR_kwDOBaVUGc5JzUqa", "number": 71, "title": "Fixed the handling of Alt keys.", "user": {"login": "im1x", "id": 1444150, "node_id": "MDQ6VXNlcjE0NDQxNTA=", "avatar_url": "https://avatars.githubusercontent.com/u/1444150?v=4", "gravatar_id": "", "url": "https://api.github.com/users/im1x", "html_url": "https://github.com/im1x", "followers_url": "https://api.github.com/users/im1x/followers", "following_url": "https://api.github.com/users/im1x/following{/other_user}", "gists_url": "https://api.github.com/users/im1x/gists{/gist_id}", "starred_url": "https://api.github.com/users/im1x/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/im1x/subscriptions", "organizations_url": "https://api.github.com/users/im1x/orgs", "repos_url": "https://api.github.com/users/im1x/repos", "events_url": "https://api.github.com/users/im1x/events{/privacy}", "received_events_url": "https://api.github.com/users/im1x/received_events", "type": "User", "user_view_type": "public", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 0, "created_at": "2023-02-12T22:51:17Z", "updated_at": "2023-02-13T02:40:35Z", "closed_at": "2023-02-13T02:40:35Z", "author_association": "CONTRIBUTOR", "active_lock_reason": null, "draft": false, "pull_request": {"url": "https://api.github.com/repos/obv-mikhail/InputBot/pulls/71", "html_url": "https://github.com/obv-mikhail/InputBot/pull/71", "diff_url": "https://github.com/obv-mikhail/InputBot/pull/71.diff", "patch_url": "https://github.com/obv-mikhail/InputBot/pull/71.patch", "merged_at": "2023-02-13T02:40:35Z"}, "body": "Most keys generate a WM_KEYDOWN(0x0100) event, but the Alt keys generate a WM_SYSKEYDOWN(0x0104) event. So we need to listen for the WM_SYSKEYDOWN event too.", "reactions": {"url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/71/reactions", "total_count": 0, "+1": 0, "-1": 0, "laugh": 0, "hooray": 0, "confused": 0, "heart": 0, "rocket": 0, "eyes": 0}, "timeline_url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/71/timeline", "performed_via_github_app": null, "state_reason": null, "score": 1.0}, {"url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/67", "repository_url": "https://api.github.com/repos/obv-mikhail/InputBot", "labels_url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/67/labels{/name}", "comments_url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/67/comments", "events_url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/67/events", "html_url": "https://github.com/obv-mikhail/InputBot/pull/67", "id": 1428474302, "node_id": "PR_kwDOBaVUGc5B0BQz", "number": 67, "title": "Convert winapi transmute to internal union handling", "user": {"login": "SonicZentropy", "id": 12196028, "node_id": "MDQ6VXNlcjEyMTk2MDI4", "avatar_url": "https://avatars.githubusercontent.com/u/12196028?v=4", "gravatar_id": "", "url": "https://api.github.com/users/SonicZentropy", "html_url": "https://github.com/SonicZentropy", "followers_url": "https://api.github.com/users/SonicZentropy/followers", "following_url": "https://api.github.com/users/SonicZentropy/following{/other_user}", "gists_url": "https://api.github.com/users/SonicZentropy/gists{/gist_id}", "starred_url": "https://api.github.com/users/SonicZentropy/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/SonicZentropy/subscriptions", "organizations_url": "https://api.github.com/users/SonicZentropy/orgs", "repos_url": "https://api.github.com/users/SonicZentropy/repos", "events_url": "https://api.github.com/users/SonicZentropy/events{/privacy}", "received_events_url": "https://api.github.com/users/SonicZentropy/received_events", "type": "User", "user_view_type": "public", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 0, "created_at": "2022-10-29T21:59:58Z", "updated_at": "2022-10-29T23:56:42Z", "closed_at": "2022-10-29T23:56:42Z", "author_association": "CONTRIBUTOR", "active_lock_reason": null, "draft": false, "pull_request": {"url": "https://api.github.com/repos/obv-mikhail/InputBot/pulls/67", "html_url": "https://github.com/obv-mikhail/InputBot/pull/67", "diff_url": "https://github.com/obv-mikhail/InputBot/pull/67.diff", "patch_url": "https://github.com/obv-mikhail/InputBot/pull/67.patch", "merged_at": "2022-10-29T23:56:42Z"}, "body": "Fixes #64 \r\n\r\nIn general, Rust 1.64 added a forced panic to `std::mem::transmute_copy` if the destination struct is larger than the source, as it is technically UB.  InputBot was using this mechanism to convert keyboard or mouse input to the INPUT_u union, which ALWAYS triggers the panic.\r\n\r\nThis fixes everything as far as I can tell, BUT it is the first unsafe code I've ever written, so please please please don't merge unless someone else is also confident it doesn't explode!  This does seem like the intended method the winapi crate wants people to use, though", "reactions": {"url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/67/reactions", "total_count": 0, "+1": 0, "-1": 0, "laugh": 0, "hooray": 0, "confused": 0, "heart": 0, "rocket": 0, "eyes": 0}, "timeline_url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/67/timeline", "performed_via_github_app": null, "state_reason": null, "score": 1.0}, {"url": "https://api.github.com/repos/obv-mikhail/InputBot/issues/66", "repository_url": 