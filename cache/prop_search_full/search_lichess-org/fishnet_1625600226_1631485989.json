[{"url": "https://api.github.com/repos/lichess-org/fishnet/issues/204", "repository_url": "https://api.github.com/repos/lichess-org/fishnet", "labels_url": "https://api.github.com/repos/lichess-org/fishnet/issues/204/labels{/name}", "comments_url": "https://api.github.com/repos/lichess-org/fishnet/issues/204/comments", "events_url": "https://api.github.com/repos/lichess-org/fishnet/issues/204/events", "html_url": "https://github.com/lichess-org/fishnet/pull/204", "id": 993940688, "node_id": "MDExOlB1bGxSZXF1ZXN0NzMyMDAxNzYw", "number": 204, "title": "Add version information to Windows executables", "user": {"login": "kriolyth", "id": 2059973, "node_id": "MDQ6VXNlcjIwNTk5NzM=", "avatar_url": "https://avatars.githubusercontent.com/u/2059973?v=4", "gravatar_id": "", "url": "https://api.github.com/users/kriolyth", "html_url": "https://github.com/kriolyth", "followers_url": "https://api.github.com/users/kriolyth/followers", "following_url": "https://api.github.com/users/kriolyth/following{/other_user}", "gists_url": "https://api.github.com/users/kriolyth/gists{/gist_id}", "starred_url": "https://api.github.com/users/kriolyth/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/kriolyth/subscriptions", "organizations_url": "https://api.github.com/users/kriolyth/orgs", "repos_url": "https://api.github.com/users/kriolyth/repos", "events_url": "https://api.github.com/users/kriolyth/events{/privacy}", "received_events_url": "https://api.github.com/users/kriolyth/received_events", "type": "User", "user_view_type": "public", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 1, "created_at": "2021-09-11T20:56:09Z", "updated_at": "2021-09-13T08:54:55Z", "closed_at": "2021-09-12T20:24:00Z", "author_association": "CONTRIBUTOR", "type": null, "active_lock_reason": null, "draft": false, "pull_request": {"url": "https://api.github.com/repos/lichess-org/fishnet/pulls/204", "ht