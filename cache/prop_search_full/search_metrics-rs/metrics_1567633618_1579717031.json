[{"url": "https://api.github.com/repos/metrics-rs/metrics/issues/65", "repository_url": "https://api.github.com/repos/metrics-rs/metrics", "labels_url": "https://api.github.com/repos/metrics-rs/metrics/issues/65/labels{/name}", "comments_url": "https://api.github.com/repos/metrics-rs/metrics/issues/65/comments", "events_url": "https://api.github.com/repos/metrics-rs/metrics/issues/65/events", "html_url": "https://github.com/metrics-rs/metrics/pull/65", "id": 547217280, "node_id": "MDExOlB1bGxSZXF1ZXN0MzYwNzQ0NzUx", "number": 65, "title": "Migrate to core::futures", "user": {"login": "dfoxfranke", "id": 4708206, "node_id": "MDQ6VXNlcjQ3MDgyMDY=", "avatar_url": "https://avatars.githubusercontent.com/u/4708206?v=4", "gravatar_id": "", "url": "https://api.github.com/users/dfoxfranke", "html_url": "https://github.com/dfoxfranke", "followers_url": "https://api.github.com/users/dfoxfranke/followers", "following_url": "https://api.github.com/users/dfoxfranke/following{/other_user}", "gists_url": "https://api.github.com/users/dfoxfranke/gists{/gist_id}", "starred_url": "https://api.github.com/users/dfoxfranke/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/dfoxfranke/subscriptions", "organizations_url": "https://api.github.com/users/dfoxfranke/orgs", "repos_url": "https://api.github.com/users/dfoxfranke/repos", "events_url": "https://api.github.com/users/dfoxfranke/events{/privacy}", "received_events_url": "https://api.github.com/users/dfoxfranke/received_events", "type": "User", "user_view_type": "public", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 1, "created_at": "2020-01-09T02:53:20Z", "updated_at": "2020-01-15T04:11:43Z", "closed_at": "2020-01-15T04:11:43Z", "author_association": "CONTRIBUTOR", "type": null, "active_lock_reason": null, "draft": false, "pull_request": {"url": "https://api.github.com/repos/metrics-rs/metrics/pulls/65", "html_url": "https://github.com/metrics-rs/metrics/pull/65", "diff_url": "https://github.com/metrics-rs/metrics/pull/65.diff", "patch_url": "https://github.com/metrics-rs/metrics/pull/65.patch", "merged_at": "2020-01-15T04:11:43Z"}, "body": "This drops all dependencies on the 'futures' crate, and upgrades to\r\nhyper-0.13 and tokio-0.2. Methods that return futures now use\r\nasync/await.\r\n\r\nSince hyper has removed hyper_run(), I have in turn removed the\r\nHttpExporter::run() method. The user should just call into_future()\r\nand spawn or await the result however they wish.", "reactions": {"url": "https://api.github.com/repos/metrics-rs/metrics/issues/65/reactions", "total_count": 0, "+1": 0, "-1": 0, "laugh": 0, "hooray": 0, "confused": 0, "heart": 0, "rocket": 0, "eyes": 0}, "timeline_url": "https://api.github.com/repos/metrics-rs/metrics/issues/65/timeline", "performed_via_github_app": null, "state_reason": null, "score": 1.0}, {"url": "https://api.github.com/repos/metrics-rs/metrics/issues/64", "repository_url": "https://api.github.com/repos/metrics-rs/metrics", "labels_url": "https://api.github.com/repos/metrics-rs/metrics/issues/64/labels{/name}", "comments_url": "https://api.github.com/repos/metrics-rs/metrics/issues/64/comments", "events_url": "https://api.github.com/repos/metrics-rs/metrics/issues/64/events", "html_url": "https://github.com/metrics-rs/metrics/pull/64", "id": 539257429, "node_id": "MDExOlB1bGxSZXF1ZXN0MzU0MjY1NDA1", "number": 64, "title": "Adds increment and decrement methods to Gauge", "user": {"login": "jeromegn", "id": 43325, "node_id": "MDQ6VXNlcjQzMzI1", "avatar_url": "https://avatars.githubusercontent.com/u/43325?v=4", "gravatar_id": "", "url": "https://api.github.com/users/jeromegn", "html_url": "https://github.com/jeromegn", "followers_url": "https://api.github.com/users/jeromegn/followers", "following_url": "https://api.github.com/users/jeromegn/following{/other_user}", "gists_url": "https://api.github.com/users/jeromegn/gists{/gist_id}", "sta