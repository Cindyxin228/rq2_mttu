[{"url": "https://api.github.com/repos/koute/cargo-web/issues/69", "repository_url": "https://api.github.com/repos/koute/cargo-web", "labels_url": "https://api.github.com/repos/koute/cargo-web/issues/69/labels{/name}", "comments_url": "https://api.github.com/repos/koute/cargo-web/issues/69/comments", "events_url": "https://api.github.com/repos/koute/cargo-web/issues/69/events", "html_url": "https://github.com/koute/cargo-web/pull/69", "id": 298852780, "node_id": "MDExOlB1bGxSZXF1ZXN0MTcwMzg0MjY1", "number": 69, "title": "fix detection of web workers as node.js runtime", "user": {"login": "jlagarden", "id": 6525868, "node_id": "MDQ6VXNlcjY1MjU4Njg=", "avatar_url": "https://avatars.githubusercontent.com/u/6525868?v=4", "gravatar_id": "", "url": "https://api.github.com/users/jlagarden", "html_url": "https://github.com/jlagarden", "followers_url": "https://api.github.com/users/jlagarden/followers", "following_url": "https://api.github.com/users/jlagarden/following{/other_user}", "gists_url": "https://api.github.com/users/jlagarden/gists{/gist_id}", "starred_url": "https://api.github.com/users/jlagarden/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/jlagarden/subscriptions", "organizations_url": "https://api.github.com/users/jlagarden/orgs", "repos_url": "https://api.github.com/users/jlagarden/repos", "events_url": "https://api.github.com/users/jlagarden/events{/privacy}", "received_events_url": "https://api.github.com/users/jlagarden/received_events", "type": "User", "user_view_type": "public", "site_admin": false}, "labels": [], "state": "closed", "locked": false, "assignee": null, "assignees": [], "milestone": null, "comments": 1, "created_at": "2018-02-21T06:08:45Z", "updated_at": "2018-02-21T15:47:48Z", "closed_at": "2018-02-21T15:47:48Z", "author_association": "NONE", "active_lock_reason": null, "draft": false, "pull_request": {"url": "https://api.github.com/repos/koute/cargo-web/pulls/69", "html_url": "https://github.com/koute/cargo-web/pull/69", "diff_url": "https://github.com/koute/cargo-web/pull/69.diff", "patch_url": "https://github.com/koute/cargo-web/pull/69.patch", "merged_at": "2018-02-21T15:47:48Z"}, "body": "When trying to use a wasm module and its javascript wrapper inside a web worker, the wrapper will call ```require( \"fs\" )``` and try to load the wasm module from a local file instead of loading it from the server. The wrapper wrongly assumes that the js code is being executed in a node.js runtime.\r\n\r\nThat is because when executing the wrapper in a web worker in the browser ```window``` is ```undefined```. \r\n\r\nPlease merge this PR to enable using cargo web with web workers.\r\n", "reactions": {"url": "https://api.github.com/repos/koute/cargo-web/issues/69/reactions", "total_count": 0, "+1": 0, "-1": 0, "laugh": 0, "hooray": 0, "confused": 0, "heart": 0, "rocket": 0, "eyes": 0}, "timeline_url": "https://api.github.com/repos/koute/cargo-web/issues/69/timeline", "performed_via_github_app": null, "state_reason": null, "score": 1.0}, {"url": "https://api.github.com/repos/koute/cargo-web/issues/62", "repository_url": "https://api.github.com/repos/koute/cargo-web", "labels_url": "https://api.github.com/repos/koute/cargo-web/issues/62/labels{/name}", "comments_url": "https://api.github.com/repos/koute/cargo-web/issues/62/comments", "events_url": "https://api.github.com/repos/koute/cargo-web/issues/62/events", "html_url": "https://github.com/koute/cargo-web/pull/62", "id": 293632647, "node_id": "MDExOlB1bGxSZXF1ZXN0MTY2NjEzMjc2", "number": 62, "title": "Improve unpacking performance with buffered IO", "user": {"login": "lnicola", "id": 308347, "node_id": "MDQ6VXNlcjMwODM0Nw==", "avatar_url": "https://avatars.githubusercontent.com/u/308347?v=4", "gravatar_id": "", "url": "https://api.github.com/users/lnicola", "html_url": "https://github.com/lnicola", "followers_url": "https://api.github.com/users/lnicola/followers", "following_url": "https://api.github.com/users/lnicola/following{/o